<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
    "http://localhost:8080/config/ibatis-3-mapper.dtd">

<mapper namespace="entidad.seguros.SolicitudesOrclTx">

	<update id="actualizaCreditoTemp" parameterType="java.util.HashMap">
		UPDATE ESIACOM.CREDITO_TEMP SET
		ESTATUS = #{estatus},
		DES_ESTATUS = #{desEstatus}
		WHERE ID = #{id}
	</update>

</mapper>
